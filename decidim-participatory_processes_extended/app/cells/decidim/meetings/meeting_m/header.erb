<div class="card__header">
  <% if has_link_to_resource? %>
    <%= link_to resource_path + "#subsubcontent", class: "card__link" do %>
      <div class="card__title">
        <%= title %>
      </div>
    <% end %>
  <% else %>
    <div class="card__link">
      <div class="card__title">
        <%= title %>
      </div>
    </div>
  <% end %>
  <div class="meeting-bottom-decor"></div>
</div>
